{
  "name": "@deno/cache-dir",
  "version": "0.0.0",
  "tasks": {
    "test": "deno test --allow-read --allow-write --allow-net --allow-env",
    "build": "deno task wasmbuild",
    "wasmbuild": "deno run -A jsr:@deno/wasmbuild@0.16.0 --sync --features wasm"
  },
  "lint": {
    "rules": {
      "include": [
        "no-console"
      ]
    }
  },
  "publish": {
    "exclude": [
      "Cargo.lock",
      "rs_lib",
      "**/*.toml"
    ]
  },
  "exclude": [
    "target"
  ],
  "exports": "./mod.ts",
  "imports": {
    "@deno/graph": "jsr:@deno/graph@^0.69.7",
    "@std/assert": "jsr:@std/assert@^0.223",
    "@std/fmt": "jsr:@std/fmt@^0.223",
    "@std/fs": "jsr:@std/fs@^0.223",
    "@std/io": "jsr:@std/io@^0.223",
    "@std/path": "jsr:@std/path@^0.223"
  }
}
